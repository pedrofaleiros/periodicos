generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Periodico {
  id             String  @id
  titulo         String
  area           String?
  linguagem      String?
  issn           String?
  revisado_pares Boolean @default(false)

  created_at DateTime @default(now())
}
